{
  "SwaggerUrls": {
    "test-api": "/swagger/v1/swagger.json",
    "customer-sample": "http://localhost:5001/swagger/v1/swagger.json"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/Customer/Brands",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/api/Customer/Brands",
      "UpstreamHttpMethod": [ "Get" ]
    },

    {
      "DownstreamPathTemplate": "/api/Customer/Brand/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/api/Customer/Brand/{everything}",
      "UpstreamHttpMethod": [ "Get" ]
    }
  ]
}
